# Add content to .gitignore to exclude unnecessary files
cat > .gitignore << 'EOF'
# Django
*.log
*.pot
*.pyc
__pycache__/
local_settings.py
db.sqlite3
db.sqlite3-journal
media

# Virtual Environment
esqro_env/
venv/
env/

# macOS
.DS_Store

# IDE
.vscode/
.idea/

# Static files (if using collectstatic)
staticfiles/

# Environment variables
.env

# Byte-compiled / optimized / DLL files
*.py[cod]
*$py.class

# Distribution / packaging
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg
MANIFEST
EOF